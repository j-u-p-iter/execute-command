    "test": "jupiter-scripts test",
    "changePermission": "chmod +x dist/lib/*.js"
  },
  "devDependencies": {
    "@j.u.p.iter/jupiter-scripts": "^1.16.0",
    "await-to-js": "^2.1.1",
    "commander": "^5.1.0",
    "os": "^0.1.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "concat-stream": "^2.0.0"
  }
}
